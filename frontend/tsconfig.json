{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "types": ["./svg.d.ts"],
    "paths": {
      "BtApi/*": [ "src/api_helper/*"],
      "BtComponents/*": [ "src/components/*"],
      "BtContexts/*": [ "src/contexts/*"],
      "BtHooks/*": [ "src/hooks/*"],
      "BtStyles/*": [ "src/styles/*"],
      "BtTypes/*": [ "src/types/*"],
      "BtTemplates/*": [ "src/templates/*"],
      "BtRoutes": [ "src/routes/index.ts"],
      "BtIcons": [ "src/icons/index.ts"],
    },
    "baseUrl": "./",
    "rootDir": "./src",
    "outDir": "./static/frontend",
    "declaration": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["src", "svg.d.ts"]
}
